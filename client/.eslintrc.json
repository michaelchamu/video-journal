{
    "extends": "airbnb",
    "plugins": ["react", "jsx-ally", "import"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": "off"
    }
}
